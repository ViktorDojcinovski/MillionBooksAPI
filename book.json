        "2019-12-29"
      ]
    },
    "author": {
      "type": "Object",
      "value": {
        "firstName": {
          "type": "faker",
          "value": "name.firstName"
        },
        "lastName": {
          "type": "faker",
          "value": "name.lastName"
        },
        "gender": {
          "type": "faker",
          "value": "name.gender"
        }
      }
    }
